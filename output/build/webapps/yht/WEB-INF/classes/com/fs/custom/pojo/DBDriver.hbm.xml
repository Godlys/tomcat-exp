<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.fs.custom.pojo">
	<class name="DBDriver" table="FS_DB_DRIVER">
		<id name="id" column="D_ID" type="java.lang.Integer">
			<generator class="increment" />
		</id>
		<property name="name" column="D_NAME" type="java.lang.String" length="64"/>
		<property name="clazz" column="D_CLASS" type="java.lang.String" length="256"/>
		<property name="url" column="D_URL" type="java.lang.String" length="256"/>
	</class>
</hibernate-mapping>

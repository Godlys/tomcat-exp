<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="">
	<class name="com.fs.hospital.pojo.SyncParam" table="FS_SYNC_PARAM"><!-- 儿童 -->
		<id name="id" type="java.lang.Integer">
			<column name="SYNC_ID"></column>
			<generator class="increment"></generator>
		</id>
		<property name="title" column="TITLE" type="java.lang.String" />
		<property name="msgContent" column="MSG_CONTENT" type="java.lang.String" />
		<property name="cycle" column="CYCLE" type="java.lang.Integer" />
		<property name="phone" column="PHONE" type="java.lang.String" />
		<property name="syncStatus" column="SYNC_STATUS" type="java.lang.Integer" />
		<property name="sqlContent" column="SQL_CONENT" type="java.lang.String" />
	</class>
</hibernate-mapping>
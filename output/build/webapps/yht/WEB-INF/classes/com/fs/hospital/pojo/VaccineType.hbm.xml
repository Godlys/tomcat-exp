<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="">
	<class name="com.fs.hospital.pojo.VaccineType" table="FS_VACCINETYPE"><!-- 疫苗类型 -->
		<id name="id" type="java.lang.Integer">
			<column name="VACC_ID"></column>
			<generator class="increment"></generator>
		</id>
		<property name="name" column="VACC_NAME" type="java.lang.String" />
		<property name="descContent" column="VACC_DESCCONTENT" type="java.lang.String" />
		<property name="addTime" column="VACC_ADDTIME" type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>
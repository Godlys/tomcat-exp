<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="">
	<class name="com.fs.hospital.pojo.ReceivedDefinition" table="FS_RECEIVED_DEFINITION">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="increment">
			</generator>
		</id>
		<property name="receivedMsg" column="RECEIVED_MSG" type="java.lang.String" />
		<property name="autoSend" column="AUTO_SEND" type="java.lang.String" />
	</class>
</hibernate-mapping>	
